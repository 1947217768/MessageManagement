@{
    ViewData["Title"] = "用户管理";
}
@section Search{
    <tr>
        <td style="width:200px">登录名:</td>
        <td style="width:400px"><input type="text" id="SloginName" name="SloginName" class="layui-input"></td>
        <td style="width:200px">用户名:</td>
        <td style="width:400px"><input type="text" id="SuserName" name="SuserName" class="layui-input"></td>
        <td style="width:200px">手机号:</td>
        <td style="width:400px"><input type="text" id="SuserPhone" name="SuserPhone" class="layui-input"></td>
    </tr>
    <tr>
        <td>邮箱:</td>
        <td><input type="text" id="SuserEmail" name="SuserEmail" class="layui-input"></td>
        <td>备注:</td>
        <td><input type="text" id="Sremarks" name="Sremarks" class="layui-input"></td>
        <td>状态:</td>
        <td style="text-align:left">
            <input type="checkbox" name="BisLock" id="BisLock" lay-filter="BisLock" lay-skin="switch" lay-text="是|否" value="1" />
        </td>
    </tr>
}
@section Button{
    @*按钮扩展*@
}
<script type="text/html" id="IsLock">
    {{#  if(d.BisLock ===true){ }}
    <input type="checkbox" name="IsLock" value="{{d.Id}}" lay-filter="IsLock" lay-skin="switch" lay-text="是|否" checked>
    {{#  } else{ }}
    <input type="checkbox" name="IsLock" value="{{d.Id}}" lay-filter="IsLock" lay-skin="switch" lay-text="是|否">
    {{#  }}}
</script>
@section Scripts{
    <script type="text/javascript" src="~/layui/layui.js"></script>
    <script src="~/Admin/UserInfo/Index.js?_=@DateTime.Now.ToString(" yyyyMMddHHmmss")"></script>
}
