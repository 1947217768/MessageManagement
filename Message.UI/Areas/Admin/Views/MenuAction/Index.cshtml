@{
    ViewData["Title"] = "菜单权限";
}
@section Search{
    <tr>
        <td style="width:200px">菜单名称:</td>
        <td style="width:400px"><input type="text" id="SmenuName" name="SmenuName" class="layui-input"></td>
        <td style="width:200px">控制名称:</td>
        <td style="width:400px"><input type="text" id="ScontrollerName" name="ScontrollerName" class="layui-input"></td>
        <td style="width:200px">说明:</td>
        <td style="width:400px"><input type="text" id="Sexplain" name="Sexplain" class="layui-input"></td>
    </tr>
    <tr>
        <td style="width:200px">备注:</td>
        <td style="width:400px"><input type="text" id="Sremarks" name="Sremarks" class="layui-input"></td>
    </tr>
}
@section Scripts{
    <script type="text/javascript" src="/layui/layui.js"></script>
    <script type="text/javascript" src="/Admin/MenuAction/Index.js?_=@DateTime.Now.ToString("yyyyMMddHHmmss")"></script>
}
